<?xml version="1.0"?>
<robot name="six_wheel_ugv">

  <link name="base_footprint" />
  <link name="base_link">
    <visual>
      <geometry>
        <box size="1.8 1.0 0.8"/>
      </geometry>
      <material name="ChassisColor">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="1.8 1.0 0.8"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="120"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="12" iyy="12" izz="20" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="wheel_left_0">
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
      <material name="Black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
    </collision>
    <inertial>
      <mass value="5"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.1" iyy="0.1" izz="0.2" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="wheel_left_1">
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
      <material name="Black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
    </collision>
    <inertial>
      <mass value="5"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.1" iyy="0.1" izz="0.2" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="wheel_left_2">
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
      <material name="Black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
    </collision>
    <inertial>
      <mass value="5"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.1" iyy="0.1" izz="0.2" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="wheel_right_0">
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
      <material name="Black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
    </collision>
    <inertial>
      <mass value="5"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.1" iyy="0.1" izz="0.2" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="wheel_right_1">
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
      <material name="Black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
    </collision>
    <inertial>
      <mass value="5"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.1" iyy="0.1" izz="0.2" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="wheel_right_2">
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
      <material name="Black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.08"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
    </collision>
    <inertial>
      <mass value="5"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.1" iyy="0.1" izz="0.2" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <link name="lidar_front">
    <visual>
      <geometry>
        <cylinder radius="0.07" length="0.08"/>
      </geometry>
      <material name="Blue">
        <color rgba="0.1 0.3 0.8 1"/>
      </material>
    </visual>
  </link>

  <link name="lidar_back">
    <visual>
      <geometry>
        <cylinder radius="0.07" length="0.08"/>
      </geometry>
      <material name="Blue">
        <color rgba="0.1 0.3 0.8 1"/>
      </material>
    </visual>
  </link>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 0"/>
  </joint>
  <joint name="joint_wheel_left_0" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left_0"/>
    <origin xyz="-0.8 -0.55 -0.35" rpy="0 0 1.57"/>
    <axis xyz="1 0 0"/>
  </joint>

  <joint name="joint_wheel_left_1" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left_1"/>
    <origin xyz="0 -0.55 -0.35" rpy="0 0 1.57"/>
    <axis xyz="1 0 0"/>
  </joint>

  <joint name="joint_wheel_left_2" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left_2"/>
    <origin xyz="0.8 -0.55 -0.35" rpy="0 0 1.57"/>
    <axis xyz="1 0 0"/>
  </joint>

  <joint name="joint_wheel_right_0" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right_0"/>
    <origin xyz="0.8 0.55 -0.35" rpy="0 0 1.57"/>
    <axis xyz="1 0 0"/>
  </joint>

  <joint name="joint_wheel_right_1" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right_1"/>
    <origin xyz="0 0.55 -0.35" rpy="0 0 1.57"/>
    <axis xyz="1 0 0"/>
  </joint>

  <joint name="joint_wheel_right_2" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right_2"/>
    <origin xyz="-0.8 0.55 -0.35" rpy="0 0 1.57"/>
    <axis xyz="1 0 0"/>
  </joint>

  <joint name="joint_lidar_front" type="fixed">
    <parent link="base_link"/>
    <child link="lidar_front"/>
    <origin xyz="0.82 0 0.44" rpy="0 0 0"/>
  </joint>

  <joint name="joint_lidar_back" type="fixed">
    <parent link="base_link"/>
    <child link="lidar_back"/>
    <origin xyz="-0.82 0 0.44" rpy="0 0 3.14159"/>
  </joint>

  

  <!-- Gazebo Plugins -->
  <gazebo reference="lidar_front">
    <sensor type="gpu_ray" name="front_lidar_sensor">
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>640</samples>
            <resolution>1</resolution>
            <min_angle>-1.047</min_angle>
            <max_angle>1.047</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.1</min>
          <max>30.0</max>
        </range>
      </ray>
      <plugin name="gazebo_ros_gpu_lidar" filename="libgazebo_ros_gpu_lidar.so">
        <topicName>/lidar_front/scan</topicName>
        <frameName>lidar_front</frameName>
      </plugin>
    </sensor>
  </gazebo>

  <gazebo reference="lidar_back">
    <sensor type="gpu_ray" name="rear_lidar_sensor">
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>640</samples>
            <resolution>1</resolution>
            <min_angle>-1.047</min_angle>
            <max_angle>1.047</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.1</min>
          <max>30.0</max>
        </range>
      </ray>
      <plugin name="gazebo_ros_gpu_lidar" filename="libgazebo_ros_gpu_lidar.so">
        <topicName>/lidar_back/scan</topicName>
        <frameName>lidar_back</frameName>
      </plugin>
    </sensor>
  </gazebo>

  <gazebo reference="camera_front">
    <sensor type="camera" name="front_camera_sensor">
      <camera>
        <horizontal_fov>2.094</horizontal_fov>
        <image>
          <width>640</width>
          <height>480</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.1</near>
          <far>50</far>
        </clip>
      </camera>
      <plugin name="gazebo_ros_camera" filename="libgazebo_ros_camera.so">
        <cameraName>front_camera</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>camera_front</frameName>
      </plugin>
    </sensor>
  </gazebo>

  <gazebo reference="camera_back">
    <sensor type="camera" name="rear_camera_sensor">
      <camera>
        <horizontal_fov>2.094</horizontal_fov>
        <image>
          <width>640</width>
          <height>480</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.1</near>
          <far>50</far>
        </clip>
      </camera>
      <plugin name="gazebo_ros_camera" filename="libgazebo_ros_camera.so">
        <cameraName>rear_camera</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>camera_back</frameName>
      </plugin>
    </sensor>
  </gazebo>

</robot>
